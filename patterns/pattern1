import java.util.*;
public class pattern1 {
    public static void main(String[]args){
        Scanner scn = new Scanner(System.in);
        int n = scn.nextInt();
        for(int i = 1; i<=n; i++){
            for(int a=1;a<=i;a++){
                System.out.print("*");
                
                
            }
            System.out.println();

        }
    }
    
}
